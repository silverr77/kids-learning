{
  "expo": {
    "name": "Learn for Kids",
    "slug": "learn-for-kids",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "extra": {
      "eas": {
        "projectId": "placeholder-project-id"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.learnforkids.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#4A90E2"
      },
      "package": "com.learnforkids.app"
    },
    "web": {
    },
    "plugins": [
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ]
    ],
    "scheme": "learnforkids"
  }
}

