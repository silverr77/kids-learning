{
  "expo": {
    "name": "Kids Learning",
    "slug": "kids-learning",
    "version": "1.4.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mustaphabkl.kidslearning",
      "buildNumber": "1",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#4A90E2"
      },
      "package": "com.mustaphabkl.kidslearning",
      "permissions": [
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {},
    "plugins": [
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3002057065219882~5172338492",
          "iosAppId": "ca-app-pub-3002057065219882~5172338492"
        }
      ]
    ],
    "scheme": "kidslearning",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e15bfc20-a0f0-4bd5-8cb2-8200e8163cd5"
      }
    }
  }
}
